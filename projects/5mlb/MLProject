name: 5mla

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      train_path: path
      model_param1: {type: int, default: 1000}
    command: "python train.py {train_path} {model_param1}"
  etl_predict:
    parameters:
      test_path_in: path
      predict_path_out: path
    command: "python etl_predict.py {test_path_in} {predict_path_out}"
